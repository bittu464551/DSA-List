class Solution {
    public int maximumWealth(int[][] accounts) {
        int max_wealth=0;
        for(int i=0;i<accounts.length;i++){
            int max=0;
            /*for(int j=0;j<accounts[i].length;j++){
                max+=accounts[i][j];
            }*/
            if(max>max_wealth){
                max_wealth=max;
            }
        }
        return max_wealth;
    }
}
